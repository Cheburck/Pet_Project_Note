      default: null
    volumes:
      - type: bind
        source: D:\Projects C#\Pet_Project_Note\Note
        target: C:\app
        bind:
          create_host_path: true
      - type: bind
        source: D:\Projects C#\Pet_Project_Note\Note
        target: C:\src
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Ришат\onecoremsvsmon\17.9.20214.3421
        target: C:\remote_debugger
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Ришат\.nuget\packages\
        target: c:\.nuget\packages
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages
        target: c:\.nuget\fallbackpackages
        read_only: true
        bind:
          create_host_path: true
      - type: bind
